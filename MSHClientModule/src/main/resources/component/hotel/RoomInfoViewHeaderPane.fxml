<?xml version="1.0" encoding="UTF-8"?>

<?import component.titlelabel.TitleLabel?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="225.0" prefWidth="1000.0" spacing="42.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.componentcontroller.order.ClientOrderSearchPaneController">
    <padding>
        <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
    </padding>
    <effect>
        <DropShadow blurType="GAUSSIAN" color="#40404080" radius="1.0" />
    </effect>
    <children>
        <TitleLabel text="房间信息">
         <VBox.margin>
            <Insets />
         </VBox.margin></TitleLabel>
        <HBox prefHeight="16.0" prefWidth="950.0" spacing="16.0">
         <children>
            <Label fx:id="AllRoomLabel" prefHeight="20.0" prefWidth="52.0" text="全部房间" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="PingFang SC Regular" size="13.0" />
               </font>
            </Label>
            <Label prefHeight="20.0" prefWidth="5.0" text="|">
               <font>
                  <Font name="PingFang SC Regular" size="13.0" />
               </font>
            </Label>
            <Label fx:id="AvailableRoomLabel" prefHeight="17.0" prefWidth="56.0" text="可用房间" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="PingFang SC Regular" size="13.0" />
               </font>
            </Label>
         </children>
        </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label layoutX="170.0" text="Label" />
         </children>
      </HBox>
    </children>
</VBox>
